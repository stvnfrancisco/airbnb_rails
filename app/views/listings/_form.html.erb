<%= render 'layouts/errors', object: @listing %>

<%= bootstrap_form_for @listing do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :description %>
  <%= f.text_field :location %>
  <%= f.number_field :rate %>
  <%= f.file_field :avatar %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.submit %>
<% end %>
